

<h2>User</h2>

<h4>Account</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <h2>@ViewData["Test"].</h2>
        <form action="~/Users/Create" method="post">
            <div class="form-group">
                <input name="Id" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Username" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Password" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Email" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Name1" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Name2" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Surname" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Sex" class="form-control" />
            </div>
            <div class="form-group">
                <input name="Key" class="form-control" />
            </div>
            <div class="form-group">
                <input name="AccountType" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


